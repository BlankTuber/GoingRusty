{
    "manifest_version": 3,
    "name": "Bookmark Sorter",
    "version": "1.0",
    "description": "Sort and filter your bookmarks by tags or categories.",
    "permissions": ["bookmarks"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
        }
    }
}
